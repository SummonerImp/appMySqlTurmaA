<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <meta charset="utf-8" />
    <title>App Turma A - @ViewBag.Title</title>
    <link href="~/Assets/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Css/sb-admin-2.css" rel="stylesheet" />
    <link href="~/Assets/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
    <script src="~/Assets/jquery/jquery.min.js"></script>
    <script src="~/Assets/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/Js/sb-admin-2.js"></script>
    <script src="~/Assets/metisMenu/metisMenu.min.js"></script>
</head>
<body>
    <div id="wrapper">
        <nav class="navbar navbar-default navbar-static-top" role="navigation" style="margin-bottom:0">
            <ul class="nav navbar-top-links navbar-right">
                <li class="dropdown">
                    <a class="dropdown-toggle">
                        <i class="fa fa-home fa-fw"></i>Início
                    </a>
                </li>
                <li class="dropdown">
                    <a class="dropdown-toggle" data-target="dropdown" href="">
                        <i class="fa fa-info-circle fa-fw"></i>Quem somos?
                    </a>
                </li>
                <li class="dropdown">
                    <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                        <i class="fa fa-bell fa-fw"></i>Serviços<i class="fa fa-caret-down"></i>
                    </a>
                    <ul class="dropdown-menu dropdown-alerts">
                        <li>
                            <a href="/Cliente/Create">
                                <div>
                                    <i class="fa fa-users fa-fw"></i>Cadastro de Clientes
                                </div>
                            </a>
                        </li>
                        <li>
                            <a href="">
                                <div>
                                    <i class="fa fa-users fa-fw"></i>Cadastro de Vendedor
                                </div>
                            </a>
                        </li>
                    </ul>
                </li>
            </ul>

            <div class="navbar-default sidebar" role="navigation">
                <div class="sidebar-nav navbar-collapse">
                    <ul class="nav" id="side-menu">
                        <li class="sidebar-search">
                            <div class="input-group custom-search-form">
                                <input type="text" class="form-control" placeholder="Procurar..."/>
                                <span class="input-group-btn">
                                    <button class="btn btn-default" type="button">
                                        <i class="fa fa-search"></i>
                                    </button>
                                </span>
                            </div>
                        </li>
                        <li>
                            <a href="#">
                                <i class="fa fa-home fa-fw"></i>Principal
                            </a>
                        </li>
                        <li>
                            <a href="#">
                                <i class="fa fa-product-hunt fa-fw"></i>Produtos<span class="fa arrow"></span>
                            </a>
                            <ul class="nav nav-second-level">
                                <li>
                                    <a href="#">
                                        <i class="fa fa-check fa-fw" style="color:#f0ad4e"></i>Categoria
                                    </a>
                                </li>
                                <li>
                                    <a href="#">
                                        <i class="fa fa-search fa-fw" style="color:#f6282c"></i>Buscar Produto
                                    </a>
                                </li>
                                <li>
                                    <a href="#">
                                        <i class="fa fa-product-hunt fa-fw" style="color: #f6282c"></i>Produto
                                    </a>
                                </li>
                                <li>
                                    <a href="#">
                                        <i class="fa fa-sort-numeric-asc fa-fw" style="color:#f6282c"></i>Estoque
                                    </a>
                                </li>
                            </ul>
                        </li>
                        <li>
                            <a href="#">
                                <i class="fa fa-user-secret fa-fw"></i>Clientes<span class="fa arrow"></span>
                            </a>
                            <ul class="nav nav-second-level">
                                <li>
                                    <a href="/Cliente/List">
                                        <i class="fa fa-search fa-fw" style="color:Highlight"></i>Listar
                                    </a>
                                </li>
                                <li>
                                    <a href="/Cliente/Create">
                                        <i class="fa fa-user-plus fa-fw" style="color:Highlight"></i>Cadastrar
                                    </a>
                                </li>
                                <li>
                                    <a href="#">
                                        <i class="fa fa-refresh fa-fw" style="color:Highlight"></i>Atualizar
                                    </a>
                                </li>
                                <li>
                                    <a href="~/Cliente/DeleteBusca">
                                        <i class="fa fa-trash fa-fw" style="color:Highlight"></i>Excluir
                                    </a>
                                </li>
                            </ul>
                        </li>
                    </ul>
                </div>
            </div>

        </nav>
        <div id="page-wrapper">
            @RenderBody()
        </div>
    </div>
    @Scripts.Render("~/bundles/inputmask")
    @RenderSection("scripts", required: false)
</body>
</html>
